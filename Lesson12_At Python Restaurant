import random

dishes_string = input('What would you like? ')
# dishes_string = 'ham, spam, eggs, eggs, sausage, meatballs, spam, spam'

dishes_string.strip()
dishes_list = dishes_string.title().split(',')
unique_dishes = set(dishes_list)

for dishes_list in unique_dishes:
    random_integer = random.randint(0, 1000)
    print(dishes_list.ljust(20,'.') + str(random_integer)+'min')
